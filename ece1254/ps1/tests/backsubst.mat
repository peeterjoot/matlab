clear all ;
U = [2 2 2 ; 0 1 2 ; 0 0 2 ] ; 
b = [ 1 ; 2 ; 3 ] ; 
x = backsubst( U, b ) ;
expect = [ 0 ; -1 ; 3/2 ] ;

if ( x ~= expect )
   disp( x ) ;
   disp( expect ) ;
   error( 'backsubst:tc1', 'x != expect' ) ;
end
 
